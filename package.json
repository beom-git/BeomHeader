{
  "name": "beom-header",
  "displayName": "Beom Header",
  "description": "Insert standardized file headers (shebang, copyright, historyâ€¦) for various file types",
  "version": "1.1.4",
  "publisher": "seongbeom",
  "license": "MIT",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/beom-git/BeomHeader.git"
  },
  "bugs": {
    "url": "https://github.com/beom-git/BeomHeader/issues"
  },
  "homepage": "https://github.com/beom-git/BeomHeader#readme",
  "activationEvents": [
    "onCommand:fileHeader.insert",
    "onCommand:fileHeader.insertVersion",
    "onCommand:fileHeader.insertTodo"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Beom Header Settings",
      "properties": {
        "beomHeader.shebangPerLanguage": {
          "type": "object",
          "description": "Mapping of shebang per language",
          "default": {
            "python": "/usr/bin/env python3",
            "bash": "/bin/bash"
          }
        },
        "beomHeader.projectName": {
          "type": "string",
          "description": "Project name",
          "default": "MyProject"
        },
        "beomHeader.copyrightStartYears": {
          "type": "string",
          "description": "Copyright start year",
          "default": "2023"
        },
        "beomHeader.company": {
          "type": "string",
          "description": "Company name",
          "default": "YourCompany"
        }
      }
    },
    "commands": [
      {
        "command": "fileHeader.insert",
        "title": "Insert File Header"
      },
      {
        "command": "fileHeader.insertVersion",
        "title": "Insert Version Entry"
      },
      {
        "command": "fileHeader.insertTodo",
        "title": "Insert TODO Entry"
      }
    ],
    "keybindings": [
      {
        "command": "fileHeader.insert",
        "key": "ctrl+alt+h",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.5.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.0.0"
  },
  "dependencies": {
    "code": "^5.2.4",
    "yo": "^5.1.0"
  }
}
